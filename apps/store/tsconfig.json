{
	"extends": "@repo/typescript-config/nextjs.json",
	"compilerOptions": {
		"ignoreDeprecations": "6.0",
		"baseUrl": ".",
		"paths": {
			"@/*": ["./*"]
		}
	},
	"include": [
		"next-env.d.ts",
		"**/*.ts",
		"**/*.tsx",
		".next/types/**/*.ts",
		"types/**/*.d.ts"
	],
	"exclude": ["node_modules"]
}
